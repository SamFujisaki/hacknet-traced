    <AddIRCMessage Author="Mentel" TargetComp="newIRC" Delay="6.0">hang on a sec...</AddIRCMessage>
    <AddIRCMessage Author="Mentel" TargetComp="newIRC" Delay="9.0">there. new missions for you guys. have a go at them and i'll try and get us back up</AddIRCMessage>
    <AddMissionToHubServer MissionFilepath="Missions/Ghosts/01/01a.xml" TargetComp="newIRC"/>
    <AddMissionToHubServer MissionFilepath="Missions/Ghosts/01/01b.xml" TargetComp="newIRC" AssignmentTag="UniteSystem"/>
    <AddMissionToHubServer MissionFilepath="Missions/Ghosts/01/01c.xml" TargetComp="newIRC" AssignmentTag="c0deMagick"/>
  </Action>
