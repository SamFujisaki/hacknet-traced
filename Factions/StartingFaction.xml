<CustomFaction name="Ghosts" id="ghostsFac" playerVal="0">
  <Action ValueRequired="1">
    <AddIRCMessage Author="Mentel" TargetComp="newIRC" Delay="2.5">sorry everyone, had to do it. i couldnt afford to take any chances.</AddIRCMessage>
    <AddIRCMessage Author="UniteSystem" TargetComp="newIRC" Delay="3.5">it's... it's fine</AddIRCMessage>
